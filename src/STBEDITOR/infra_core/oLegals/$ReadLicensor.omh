	Text: {<phone_sales>+44 1442 41 2232</phone_sales>}
	Text: {<phone_support>+44 1442 41 2232</phone_support>}
	Text: {<email_sales>rmostyn@multidimensionalmoney.net</email_sales>}
	Text: {<email_support>technical_support@multidimensionalmoney.net</email_support>}
	Text: {</licensor></xml>}
	End text block
	Get text block lcContents
End If

If lcContents<>''
	Do method ParseLicensorFile (lcContents,irLicensor)
End If

Quit method irLicensor